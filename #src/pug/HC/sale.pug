                    .sale-apartments__img
                        img(src="img/reg.png" alt='')
                    .sale-apartments__right
                        .sale-apartments__conditions
                            .sale-apartments__block
                                img(src="img/icons/pl.svg")
                                b от 68,3 м²
                            .sale-apartments__block
                                img(src="img/icons/ot.svg")
                                p Без отделки
                            .sale-apartments__block
                                img(src="img/icons/kom.svg")
                                b 2
                            .sale-apartments__block
                                img(src="img/icons/et.svg")
                                b 5

                        .sale-apartments__price
                            span от 4 441 450 ₽
                            p от 65 000 ₽/м²

                        .sale-apartments__btn.button.des Показать предложение
