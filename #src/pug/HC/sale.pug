section.sale
    ._container
        .sale__title.title Квартиры в продаже
        .sale__text Можете подобрать квартиру под ваши пожелания!

        .sale__nav.sale-nav
            .sale-nav-top
                .sale-nav-top__element.active(data-mainliter="1")
                    .sale-nav-top__body Литер 1
                .sale-nav-top__element(data-mainliter="2")
                    .sale-nav-top__body Литер 2
                .sale-nav-top__element(data-mainliter="all")
                    .sale-nav-top__body 10 квартир  в продаже

            .sale__nav-bottom
                .sale__nav-bottom__element(data-mainroom="0")
                    .sale__nav-bottom__body Студия
                .sale__nav-bottom__element(data-mainroom="1")
                    .sale__nav-bottom__body 1-комнатная
                .sale__nav-bottom__element(data-mainroom="2")
                    .sale__nav-bottom__body 2-комнатная
                .sale__nav-bottom__element(data-mainroom="3")
                    .sale__nav-bottom__body 3-комнатная
                .sale__nav-bottom__element(data-mainroom="all")
                    .sale__nav-bottom__body Все




